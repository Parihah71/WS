#!/usr/bin/env phyton

import random

def main():
    """Start a number guessing game between 1-100."""
print("Guess the number!")

x = random.randint(1, 50)
guess = None

while x != guess:

    guess = int(input("Pick a number between 1 to 50:  ")) 

    if x == guess:
         print (" You a ginues.")
    else:
         print (" Wrong answer.")
   
main()

